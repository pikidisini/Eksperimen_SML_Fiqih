name: smsml_fiqih_model

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_path: {type: str, default: "data/train_FD001.txt"}
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
    command: >
      python modelling.py
      --data_path {data_path}
      --n_estimators {n_estimators}
      --max_depth {max_depth}
